CREATE TABLE calTable (
    calID TEXT PRIMARY KEY,
    calLink TEXT,
    calDesp TEXT,
    name TEXT,
    email TEXT,
    openDate INTEGER,
    closeDate INTEGER,
    startTime INTEGER,
    endTime INTEGER,
    FOREIGN KEY(room) REFERENCES rooms(room),
    FOREIGN KEY(user) REFERENCES users(user)
);

CREATE TABLE resvTable (
	resvID TEXT PRIMARY KEY,
	calID TEXT,
	name TEXT,
	email TEXT,
	forWhat TEXT,
	startTime INTEGER,
	endTime INTEGER,
	FOREIGN KEY(calID) REFERENCES calTable(calID)
);
